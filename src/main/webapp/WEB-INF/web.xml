<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>WeightRegistratrionServlet</servlet-name>
		<servlet-class>org.svearike.trainingpalweb.WeightRegistratrionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WeightRegistratrionServlet</servlet-name>
		<url-pattern>/weight/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetUsersServlet</servlet-name>
		<servlet-class>org.svearike.trainingpalweb.GetUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetUsersServlet</servlet-name>
		<url-pattern>/getUsers/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MoveWeightServlet</servlet-name>
		<servlet-class>org.svearike.trainingpalweb.MoveWeightServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MoveWeightServlet</servlet-name>
		<url-pattern>/moveWeight/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserInfoServlet</servlet-name>
		<servlet-class>org.svearike.trainingpalweb.UserInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserInfoServlet</servlet-name>
		<url-pattern>/updateUser/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetWeightStatsServlet</servlet-name>
		<servlet-class>org.svearike.trainingpalweb.GetWeightStatsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetWeightStatsServlet</servlet-name>
		<url-pattern>/getWeight/*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
